void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim){

	static uint16_t count = 0;
	HAL_GPIO_TogglePin(LD2_GPIO_Port, LD2_Pin);
	if(count==2){
		HAL_GPIO_TogglePin(LD4_GPIO_Port, LD4_Pin);
		count = 0;
	}
	count++;

}
