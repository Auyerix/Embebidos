void debounceFSM_update(void){

		switch(estadoActual){

		case BUTTON_UP:
		break;

		case BUTTON_FALLING:
		break;

		case BUTTON_DOWN:
		break;

		case BUTTON_RAISING:
		break;

		default:




		}


}
