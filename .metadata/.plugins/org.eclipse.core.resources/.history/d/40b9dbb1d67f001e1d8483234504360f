void debounceFSM_update(void){

		switch(debounceState){

		case BUTTON_UP:
			//HAL_GPIO_ReadPin(B1_GPIO_Port, B1_Pin)
		break;

		case BUTTON_FALLING:
		break;

		case BUTTON_DOWN:
		break;

		case BUTTON_RAISING:
		break;

		default:




		}


}
